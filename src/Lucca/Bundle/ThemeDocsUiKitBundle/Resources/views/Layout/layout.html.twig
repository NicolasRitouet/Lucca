<!-- Footer -->
<footer class="bg-dark font-weight-light text-white-55 py-4">
    <div id="footerLogo"></div>
    <div class="container footer-v1">
        <div class="row margin-bottom-15 footer-light">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="text-center animated fadeInLeft">
                    <h3>{{ ('footer.' ~ 'setting.general.departement.name'| setting ~ '.title')|trans }}</h3>
                </div>
            </div>
        </div>
        <div class="row justify-content-md-between mt-4">
            {{ render(controller("Lucca\\Bundle\\ContentBundle\\Controller\\RenderController::footerAction")) }}
        </div>
        <hr class="my-5 opacity-10">
        <div class="row justify-content-between">
            <div class="col-md-7">
                <p class="small font-weight-light mb-0">
